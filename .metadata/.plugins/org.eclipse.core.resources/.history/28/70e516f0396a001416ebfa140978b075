import java.util.ArrayList;
import java.util.Map;
import java.util.HashMap;


class MinStack {
    int min_index;
    ArrayList<Integer> lst;
    Map<Integer, Integer> map;
    MinStack(){
        min_index = -1;
        lst = new ArrayList<Integer>();
        map = new HashMap<Integer,Integer>();
    }
    
    public void push(int x) {
        map.put(lst.size(),min_index);
        if(lst.size() == 0){
            min_index = 0;
        }else{
            if(x < lst.get(min_index)){
                min_index = lst.size();
            }
        }
        lst.add(x);
    }

    public void pop() {
        if(lst.size()==0){return;}
        if(min_index == lst.size()-1){
            min_index = map.get(lst.size()-1);
            map.remove(lst.size()-1);
        }
        lst.remove(lst.size()-1);
    }

    public int top() {
        if(lst.size() == 0){return 0;}
        return lst.get(lst.size()-1);
    }

    public int getMin() {
        return min_index==-1?0:lst.get(min_index);
    }
}

public class Solution{
	public static void main(String arg[]){
		MinStack s = new MinStack();
		s.push(-3);
		System.out.print(s.getMin());
	}
}